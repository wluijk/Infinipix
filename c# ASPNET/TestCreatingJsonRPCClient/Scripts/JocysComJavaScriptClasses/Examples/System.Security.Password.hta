<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html xmlns="http://www.w3.org/1999/xhtml">
<!-- http://msdn2.microsoft.com/en-us/library/ms536473.aspx -->
<hta:application id="PassGenHta" applicationname="Jocys.com Password Generator" border="thick"
	borderstyle="normal" caption="yes" contextmenu="yes" icon="../System.Security.Password/Images/Icons/PassGen.ico"
	innerborder="no" maximizebutton="yes" minimizebutton="yes" navigable="no" scroll="no"
	scrollflat="no" selection="yes" showintaskbar="yes" singleinstance="yes" sysmenu="yes"
	version="1.0" windowstate="normal">
	<meta http-equiv="MSThemeCompatible" content="yes" />
<head>
	<title>Jocys.com Password Generator</title>
	<script type="text/javascript">
	window.onload = function(){
		var commandLine = PassGenHta.commandLine;
		// Set width and height of interface.
		var winWidth = 850;
		var winHeight = 652; 
		// Calculate center coordinates.
		var centerX = (screen.width/2) - (winWidth/2);
		var centerY = (screen.height/2) - (winHeight/2);
		// Resize window and move into spot.
		window.resizeTo(winWidth, winHeight);
		window.moveTo(centerX, centerY);
		// Load main password generator interface.
		var url = window.location.href.replace("Frameset","Interface");
		document.getElementById("InterfaceFrame").contentWindow.location.replace("System.Security.Password.Interface.htm");
	}
	</script>
</head>
<body style="border-width: 0; padding: 0; margin: 0; overflow: hidden;">
<!-- We need this frame to remove border from XHTML Page -->
<iframe application="yes" id="InterfaceFrame" style="width: 100%; height: 100%; background: ButtonFace;" frameborder="0"></iframe>
</body>
</html>
